#include <stdio.h>

void runningSum(int nums[], int n, int result[]) {
    result[0] = nums[0];
    for (int i = 1; i < n; i++) {
        result[i] = result[i - 1] + nums[i];
    }
}

int main() {
    int nums[] = {1, 2, 3, 4};
    int n = sizeof(nums) / sizeof(nums[0]);
    int result[n];

    runningSum(nums, n, result);

    printf("Running Sum: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", result[i]);
    }

    return 0;
}

#include <stdio.h>

void shuffle(int nums[], int n, int result[]) {
    int j = 0;
    for (int i = 0; i < n; i++) {
        result[j++] = nums[i];     
        result[j++] = nums[i + n]; 
    }
}

int main() {
    int nums[] = {2, 5, 1, 3, 4, 7};
    int n = 3;
    int result[2 * n];

    shuffle(nums, n, result);

    printf("Shuffled Array: ");
    for (int i = 0; i < 2 * n; i++) {
        printf("%d ", result[i]);
    }

    return 0;
}

#include <stdio.h>

int tribonacciHelper(int n, int memo[]) {
if (n == 0) return 0;
if (n == 1 || n == 2) return 1;


if (memo[n] != -1) return memo[n]; 

memo[n] = tribonacciHelper(n - 1, memo) + tribonacciHelper(n - 2, memo) + tribonacciHelper(n - 3, memo);
return memo[n];


}

int tribonacci(int n) {
int memo[38]; 
for (int i = 0; i <= 37; i++) memo[i] = -1;
return tribonacciHelper(n, memo);
}

int main() {
int n;
printf("Enter n: ");
scanf("%d", &n);


int result = tribonacci(n);
printf("T_%d = %d\n", n, result);

return 0;


}
